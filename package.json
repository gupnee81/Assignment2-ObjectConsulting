    "enzyme-adapter-react-15": "^1.1.0",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.5.0",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
